@model ModuloNotificaciones.Entities.Producto

@{
    ViewData["Title"] = "Detalles";
}



<div>
    <h4>Detalles Del Producto</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.Nombre) Del Producto
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.Precio)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.Precio)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.FechaRegistro)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.FechaRegistro)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.Usuario)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.Usuario.UsuarioId)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.Usuario.Nombre)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.Usuario.Nombre)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ProductoId">Editar</a> |
    <a asp-action="Index">Regresar a la Lista</a>
</div>
